type Image {
  _id: String!
  name: String!
  price: Float!
  thumbnail_url: String!
  fullsize_url: String!
}

scalar Upload

type Query {
  image(_id: String!): Image!
  images: [Image!]!
}

input NewImage {
  name: String!
  price: Float!
  file: Upload!
}

type Mutation {
  uploadImage(input: NewImage!): Boolean!
  deleteImage(_id: String!): Image!
  createCheckoutSession(photoID: String!): String!
}
